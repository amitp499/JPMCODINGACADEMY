package com.jp.lc;

public class ServiceBean {
	
	private int x;
	
	
	public ServiceBean(int x){
		
		this.x=x;
		System.out.println("in constructor");
	}
	
	public void setValueX(int x){
		
		this.x=x;
		System.out.println("in setter");
	}
	
	public void validateConfig(){
		
		if(x>100){
			System.out.println("wrong input");
		}
	}

}
